{
  "name": "hitech/indonesia-laravel",
  "description": "Indonesia Laravel - Data terbaru daerah-daerah di indonesia termasuk 38 provinsi.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "dhank77",
      "email": "d41113512@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Hitech\\IndonesiaLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hitech\\IndonesiaLaravel\\Providers\\IndonesiaLaravelServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^12.19",
    "illuminate/console": "^12.19",
    "illuminate/database": "^12.19"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "tightenco/duster": "^3.2",
    "orchestra/testbench": "^10.4"
  }
}
